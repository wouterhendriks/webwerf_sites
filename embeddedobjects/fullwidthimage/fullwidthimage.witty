[component embeddedobject]
  <div class="embeddedobject embeddedobject--fullwidthimage embeddedobject--[if isrtdpreview]preview[else]site[/if]">
    [if isrtdpreview]
      <img src="[image.link]" style="max-height: 200px;" />
      <div class="embeddedobject__type">Afbeelding over hele breedte</div>
    [else]
      <div class="emb-fullwidthimage">
        <img class="emb-fullwidthimage__image" src="[image.link]" />
      </div>
    [/if]
  </div>
[/component]
